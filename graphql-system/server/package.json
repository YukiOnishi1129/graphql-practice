{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "ts-node": "ts-node",
    "start": "nodemon ./src/index.ts",
    "watch": "ts-node-dev ./src/index.ts",
    "lint": "eslint --ext '.js,.ts' ./src/**",
    "lint-fix": "eslint --fix --ext '.js,.ts' ./src/**",
    "generate": "graphql-codegen --config codegen.yml",
    "codegen": "graphql-codegen --config codegen.yml",
    "create:model": "ts-node ./node_modules/.bin/typeorm entity:create --name",
    "db:migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate --name",
    "db:migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "db:migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "db:schema:drop": "ts-node ./node_modules/.bin/typeorm schema:drop",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "test:command": "ts-node ./src/utils/test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@Types": "src/types/*",
    "@Models": "src/models",
    "@Services": "src/services",
    "@Constants/*": "src/constants",
    "@Utils/*": "src/utils/"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-resolvers": "1.19.1",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.4",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/log4js": "^2.3.5",
    "@types/mysql": "^2.15.17",
    "@types/node": "^14.14.19",
    "@types/node-schedule": "^1.3.1",
    "@types/nodemailer": "^6.4.0",
    "@types/sinon-express-mock": "^1.3.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "graphql-tools": "^7.0.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sinon": "^9.2.4",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.24.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
